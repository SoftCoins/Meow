
num_1 = 7
num_2 = 10
num_3 = 4
summ = num_1 + num_2 + num_3
print("Сумма всех чисел:" , summ)
num_1 = 7.9
num_2 = 21.3
num_3 = float(4)
summ = num_1 + num_2 + num_3
print("Сумма всех чисел:" , summ)
num_1 = 130
num_2 = 4
num_3 = 2
multiplying = num_1 * num_2 * num_3
print("Умножение всех чисел:" , multiplying)
division =  (num_1 * num_2) // num_3
print("Деление:" , division)
