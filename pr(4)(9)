n_1 = int(input(""))
n_2 = int(input(""))
n_3 = int(input(""))

if n_1 == n_2 and n_2 == n_3 and n_1 == n_3:
    print("Равностороний")
elif n_1 == 0 or n_2 == 0 or n_3 == 0:
    print("Ошибка!")
elif n_1 == n_2 or n_1 == n_3 or n_2 == n_3:
    print("Равнобедреный")
else:
    print("Разностороний")
